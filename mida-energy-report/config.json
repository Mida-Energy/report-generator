{
  "name": "Energy Reports",
  "version": "2.1.2",
  "slug": "energy-reports",
  "description": "Generate professional PDF energy reports from Shelly EM data with Home Assistant-style UI and automatic scheduling",
  "url": "https://github.com/Mida-Energy/energy-reports",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "privileged": ["SYS_ADMIN", "SYS_RAWIO"],
  "full_access": true,
  "apparmor": false,
  "image": "ghcr.io/mida-energy/energy-reports-{arch}",
  "ports": {
    "5000/tcp": 5000
  },
  "ports_description": {
    "5000/tcp": "Web interface and API"
  },
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:chart-line",
  "panel_title": "Energy Reports",
  "panel_admin": false,
  "map": [
    "config:rw",
    "share:rw",
    "media:rw"
  ],
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "options": {},
  "schema": {}
}
